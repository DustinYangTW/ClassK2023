@model IEnumerable<MyModel_CodeFirst.Models.ReBook>

@{
    Layout = null;
}


<table class="table">
    
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Author)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeStamp)
            </td>
          
           
        </tr>
}
    </tbody>
</table>
